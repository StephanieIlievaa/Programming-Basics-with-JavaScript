function solve(age, sex) {
    age = Number(age);
   
       if (sex == "m") {
           if (age >= 16) {
             console.log("Mr.");
           }else{ 
             console.log("Master");
       }
     }else{
           if (age >= 16) {
             console.log("Ms.");
           }else{
             console.log("Miss");
           }
       }
   }    
